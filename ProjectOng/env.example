# ========================================
# CONFIGURAÇÕES DO BANCO DE DADOS
# ========================================

# Host do banco PostgreSQL
DATABASE_HOST=localhost

# Usuário do banco PostgreSQL
DATABASE_USERNAME=postgres

# Senha do banco PostgreSQL
DATABASE_PASSWORD=password

# Nome do banco de dados
DATABASE_NAME=voluntariar

# Porta do banco PostgreSQL
DATABASE_PORT=5432

# ========================================
# CONFIGURAÇÕES DA APLICAÇÃO
# ========================================

# Ambiente de execução (development, production, test)
NODE_ENV=development

# Porta onde a aplicação será executada
PORT=3333

# ========================================
# CONFIGURAÇÕES DE AUTENTICAÇÃO
# ========================================

# Chave secreta para assinatura dos tokens JWT
# IMPORTANTE: Altere esta chave em produção!
JWT_SECRET=your-super-secret-jwt-key-here-change-in-production


